# Gateway
PORT=3000

# Downstream base URLs (docker 네트워크)
AUTH_BASE_URL=http://auth:3000
EVENT_BASE_URL=http://game-event:3000

# Gateway options
GATEWAY_TIMEOUT_MS=5000
GATEWAY_FORWARD_CLAIMS=true
GATEWAY_RATE_LIMIT_TTL_SEC=60
GATEWAY_RATE_LIMIT_LIMIT=120
GATEWAY_CB_FAILURE_THRESHOLD=5
GATEWAY_CB_COOLDOWN_MS=10000
CORS_ORIGIN=*

# Access Token 검증
JWT_SECRET=nexon
JWT_ISS=auth
JWT_AUD=gateway
